service cloud.firestore {
  match /databases/{database}/documents {
    match /posts/{post}
    {
      allow read: if true;
      allow write: if request.auth.uid != null;
    }
    match /spots/{spot}
    {
      allow read: if true;
      allow write: if request.auth.uid != null;
    }
    match /users/{user} 
    {
      allow read, write: if false;
    }
  }
}